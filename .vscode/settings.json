{
    "editor.codeActionsOnSave": {
      "source.addMissingImports.ts": "always",
      "source.fixAll.eslint": "always",
      "source.fixAll.ts": "always",
      "source.sortImports": "always"
    },
    "editor.tabSize": 2,
    "explorer.compactFolders": false,
    "eslint.useFlatConfig": true,
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "css"
    ]
  }
